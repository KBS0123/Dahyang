<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="defaultFetchSize" value="100" />
		<setting name="defaultStatementTimeout" value="30" />
	</settings>
	
	<typeAliases>
		<package name="spring_Dahyang.user.model"/>
		<package name="spring_Dahyang.club.model"/>
		<package name="spring_Dahyang.feed.model"/>
		<package name="spring_Dahyang.chat.model"/>
	</typeAliases>

	<typeHandlers>
        <typeHandler handler="spring_Dahyang.chat.handler.LocalDateTimeTypeHandler" javaType="java.time.LocalDateTime"/>
    </typeHandlers>
</configuration>